{
    "name": "Zuluru/Jquery",
	"version": "0.0.1",
    "description": "Zuluru Jquery plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Zuluru\\Jquery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zuluru\\Jquery\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
